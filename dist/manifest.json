{
    "manifest_version": 3,
    "name": "Unblock Redirector",
    "version": "1.0",
    "description": "Redirects current page to archive.is",
    "permissions": ["tabs", "activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.economist.com/*", "https://www.ft.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  